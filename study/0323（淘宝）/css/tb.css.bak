*{margin:0;padding:0;}
a{text-decoration:none;font-size:12px;}
li{list-style:none;}
/* header 头部 */
header{background:#f1f1f1;border-bottom:1px solid #eeee;}
.left{float:left;}
.right{float:right;}
#header{width:1190px;height:35px;margin:0 auto;}
#header a{color:#6c6c6c;font-size:13px;}
#header a:hover{color:#ff4204;}
#header .left a{line-height:35px;}
#header .login a{color:#ff4204;}
#header .register{margin:0 20px 0 10px;}
.header_right{width:590px;float:right;}
.header_right .left>a{display:block;paddding:0 7px;border-bottom:1px solid #f1f1f1;
border-left:1px solid #f1f1f1;border-right:1px solid #f1f1f1;position:relative;z-index:6;}
.header_right>.left:hover>a{border-bottom:1px solid #fff;border-left:1px solid #e4dde4;border-right:1px solid #e4dde4;background:#fff;}
.header_select{float:right;width:7px;height:35px;background:url(../images/xiala2.png) no-repeat center;background-size:100%;margin-left:5px;}
.header_select_1{float:left;width:17px;height:35px;background:url(../images/gouwuche.png) no-repeat center;background-size:70%;}
.header_select_2{float:left;width:17px;height:35px;background:url(../images/wujiaoxing.png) no-repeat center;background-size:70%;}
.header_right>li{position:relative;}
.select_section{display:none;padding:7px 0;position:absolute;top:35px;left:0;background:#fff;border:1px solid #e4dde4;}
.select_section li {padding:7px;white-space:nowrap;height:20px;}
#header .select_section li a{line-height:20px;}
.select_section li:hover{background:#e4dde4;}
.header_right>li:hover .select_section{display:block;}
.header_right>li:hover .header_select_2{float:left;width:17px;height:35px;background:url(../images/wujiaoxing-on.png) no-repeat center;background-size:70%;}
/* section logo 导航 */
#search-section{width:1200px;height:165px;margin:0 auto;}
.search-section-top{height:130px;width:100%;margin-left:5px;}
.logo{height:130px;overflow:hidden;}
.search-box{margin:30px 0 0 75px;}
.search-box>ul>li{float:left;}
.search-box>ul>li a{font-size:12px;width:50px;height:25px;line-height:25px;text-align:center;color:#ff4204;}
.search-box .box-taobao{background:#ff4204;color:#fff;}
.search-box ul li a:hover{opacity: 0.7;}
.search{height:34px;width:620px;border:2px solid #ff4204;}
.sch-box{width:670px;height:34px;}
.sch-box a{width:34px;height:34px;margin-top:2px;color:#666666;}
.search-img{width:20px;height:20px;margin:7px 0 7px 7px;}
.search input{width:436px;height:30px;outline:none;border:1px solid #fff;}
