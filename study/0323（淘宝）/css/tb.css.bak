/* article 热卖单品 */
article{background: #f1f1f1;}
#article{width:1185px;height:622px;background:#fff;margin:0 auto;}
#article h2 i{position:absolute;top:-9px;left:520px;float:left;width:20px;height:20px;background:url(../images/qwqwq.png) no-repeat center;margin:10px 0 0 10px;}
#article h2{position:relative;float:left;font-weight:400;width:100%;height:20px;line-height:20px;margin:5px auto 15px;font-size:18px;text-align:center;color:#3c3c3c;}
.article-ul{height:70px;}
.article-ul li a{float:left;padding:0 10px;color:#6c6c6c;height:14px,line-height:14px;border-right:1px solid #e8e8e8;}
.article-ul-1{margin-left:10px;}
.article-1{width:395px;height:550px;}
.article-1-left{width:235px;height:395px;border:1px solid rgb(232,232,232);position:relative;}
.article-1-right{width:158px;height:395px;}
.article-1-right1{height:197px;border:1px solid rgb(232,232,232);position:relative;}
.article-1-right2{width:157px;height:197px;border:1px solid rgb(232,232,232);position:relative;}
.article-1 img{width:395px;height:155px;}
.article-1-left img{width:215px;height:215px;margin:10px;}
.article-1-left b,.article-1-right1 b,.article-1-right2 b{display:block;color:#f40;margin-left:10px;}
.article-1-left span{display:block;font-size:12px;height:12px;line-height:12px;color:#9c9c9c;padding:10px 0 15px 10px;}
.article-1-left p{color:#6c6c6c;font-size:13px;height:20px;line-height:20px;margin-left:10px;}
.article-1-left i{display:block;margin:50px 0 0 10px;font-style:normal;color:#9c9c9c;font-size:12px;}
.article-1-right1 img,.article-1-right2 img{width:125px;height:125px;margin:10px;}
.article-1-right1 i{display:block;margin-left: 10px;font-style:normal;color:#9c9c9c;font-size:12px;}
.article-1-right2 i{color:#fff;background:rgb(140,191,76);
font-size:12px;width:30px;height:15px;line-height:15px;margin-left:10px;}
.article-1-right2 span{font-size:12px;height:12px;line-height:12px;color:#9c9c9c;}
.hover-1{display:none;width:235px;height:225px;background:rgba(255,68,0,0.8);position:absolute;left:0;top:0;}
.hover-1 p{font-size:14px;padding-top:100px;color:#fff;margin-bottom:10px;}
.hover-1 span{display:inline;color:rgb(255,68,0);padding:5px 20px;margin-left:10px;border-radius:5px;background:#fff;}
.article-1-left:hover .hover-1{display:block;}
.article-1-left:hover {border:1px solid rgb(255,68,0);}
.hover-2{display:none;width:157px;height:137px;background:rgba(255,68,0,0.8);position:absolute;left:0;top:0;}
.hover-2 p{margin-left:10px;font-size:13px;padding-top:50px;color:#fff;margin-bottom:10px;}
.hover-2 span{display:inline;color:rgb(255,68,0);padding:5px 20px;margin-left:10px;border-radius:5px;background:#fff;}
.article-1-right1:hover .hover-2{display:block;}
.article-1-right1:hover {border:1px solid rgb(255,68,0);}
.hover-3{display:none;width:157px;height:137px;background:rgba(255,68,0,0.8);position:absolute;left:0;top:0;}
.hover-3 p{margin-left:10px;font-size:13px;padding-top:50px;color:#fff;margin-bottom:10px;}
.hover-3 span{display:inline;color:rgb(255,68,0);padding:5px 20px;margin-left:10px;border-radius:5px;background:#fff;}
.article-1-right2:hover .hover-3{display:block;}
.article-1-right2:hover {border:1px solid rgb(255,68,0);}
