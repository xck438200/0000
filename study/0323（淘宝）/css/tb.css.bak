.search a:hover{background:url(../images/xiangji-on.png) no-repeat right;background-size:70%;}
.search button{height:34px;width:118px;background:#f52b00;border:0;color:#fff;font-size:20px;line-height:34px;}
.search-nav a{color:#666666;margin:10px 10px 0 0;}
.search-section-bottom{height:35px;}
.park{width:180px;height:35px;font-size:14px;line-height:35px;color:#fff;
background:#ff6537 url(../images/fenlei.png) no-repeat right;background-size:10%;padding-left:10px;margin-left:5px;}
.search-section-bottom li{padding: 0 4px;margin: 0 3px;color:#e7dde7;line-height:28px;}
.search-section-bottom li a{display:block;line-height:35px;font-size:16px;font-weight:900;color:#3c3c3c;padding:0 4px;border-radius:14px;}
.search-section-bottom li:hover a{color: #fff;background: #FF4204;}
